# Environment Variables
# Copy this file to .env and update the values

# The application ID used to to uniquely store session and cache data, mutex locks, and more
CRAFT_APP_ID=CraftCMS--12345678-1234-1234-1234-123456789012

# The environment Craft is currently running in (dev, staging, production, etc.)
CRAFT_ENVIRONMENT=dev

# The secure key Craft will use for hashing and encrypting data
CRAFT_SECURITY_KEY=

# Database Configuration
CRAFT_DB_DRIVER=mysql
CRAFT_DB_SERVER=localhost
CRAFT_DB_PORT=3306
CRAFT_DB_DATABASE=thilacoloma_craft
CRAFT_DB_USER=craft_user
CRAFT_DB_PASSWORD=your_secure_password
CRAFT_DB_SCHEMA=public
CRAFT_DB_TABLE_PREFIX=

# General Settings
CRAFT_DEV_MODE=true
CRAFT_ALLOW_ADMIN_CHANGES=true
CRAFT_DISALLOW_ROBOTS=true

# S3 Settings (Optional)
CRAFT_S3_KEY_ID=
CRAFT_S3_SECRET=
CRAFT_S3_BUCKET=
CRAFT_S3_REGION=

# Site URL
CRAFT_SITE_URL=https://thilacoloma.be
CRAFT_BASE_URL=https://thilacoloma.be
CRAFT_BASE_PATH=/

# Email Settings
CRAFT_MAIL_FROM_EMAIL=info@thilacoloma.be
CRAFT_MAIL_FROM_NAME="Thila Coloma"
CRAFT_MAIL_REPLY_TO_EMAIL=info@thilacoloma.be
